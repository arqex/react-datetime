    "eslint": "^3.1.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1",
    "gulp-insert": "^0.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
