{
  "name": "react-datetime",
  "version": "3.0.0",
  "description": "A lightweight but complete datetime picker React.js component.",
  "homepage": "https://github.com/YouCanBookMe/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/YouCanBookMe/react-datetime"
  },
  "main": "./lib/DateTime.js",
  "module": "./src/DateTime.js",
  "typings": "./DateTime.d.ts",
  "files": [
    "DateTime.js",
    "DateTime.d.ts",
    "src",
    "css",
    "dist"
  ],
  "scripts": {
    "build": "npm run build:bundle && npm run build:lib",
    "build:bundle": "webpack -p",
    "build:lib": "rm -rf lib && babel src --out-dir lib --babelrc",
    "build:lib:watch": "npm run build:lib -- --watch",
    "dev": "webpack-dev-server --config example/webpack.config.js --devtool eval --progress --colors --hot --content-base example",
    "lint": "eslint src/ test/",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest test/tests.spec.js",
    "test:watch": "npm test --watch",
    "test:typings": "tsc -p ./typings",
    "test:snapshot": "jest test/snapshots.spec.js",
    "test:snapshot:update": "jest test/snapshots.spec.js --updateSnapshot",
    "test:all": "npm run test:typings && npm run test"
  },
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "author": "Javier Marquez",
  "license": "MIT",
  "peerDependencies": {
    "moment": ">=2.16.0",
    "react": ">=0.13",
    "react-dom": ">=0.13"
  },
  "devDependencies": {
    "@types/react": ">=15",
    "babel-cli": "^6.22.0",
    "babel-core": "^6.22.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "create-react-class": "^15.6.2",
    "enzyme": "^2.9.1",
    "eslint": "^3.1.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "moment": ">=2.16.0",
    "pre-commit": "^1.1.3",
    "react": "~15.4.0",
    "react-addons-test-utils": "~15.4.0",
    "react-dom": "~15.4.0",
    "react-onclickoutside": "^5.11.1",
    "react-test-renderer": "~15.4.0",
    "typescript": "^2.0.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.7"
  },
  "pre-commit": [
    "lint",
    "test:all"
  ]
}
