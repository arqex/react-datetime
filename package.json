  "description": "A lightweight but complete datetime picker React.js component",
  "homepage": "https://github.com/YouCanBookMe/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/YouCanBookMe/react-datetime"
  },
  "main": "./DateTime.js",
  "typings": "./DateTime.d.ts",
  "files": [
    "DateTime.js",
